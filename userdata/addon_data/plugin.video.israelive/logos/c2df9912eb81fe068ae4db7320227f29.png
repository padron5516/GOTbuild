<script type="text/javascript">
    $j = jQuery.noConflict();


    function luminanace(r, g, b) {
        var a = [r,g,b].map(function(v) {
            v /= 255;
            return (v <= 0.03928) ? v / 12.92 : Math.pow(((v + 0.055)/1.055), 2.4 );
        });
        return a[0] * 0.2126 + a[1] * 0.7152 + a[2] * 0.0722 + .05;
    }

    function hexToRgb(hex) {
        // Expand shorthand form (e.g. "03F") to full form (e.g. "0033FF")
        var shorthandRegex = /^#?([a-f\d])([a-f\d])([a-f\d])$/i;
        hex = hex.replace(shorthandRegex, function(m, r, g, b) {
            return r + r + g + g + b + b;
        });

        var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
        return result ? {
            r: parseInt(result[1], 16),
            g: parseInt(result[2], 16),
            b: parseInt(result[3], 16)
        } : null;
    }

    function GetRatioForColor(hex_bg, hex_color){
        var rgb_gb = hexToRgb(hex_bg);
        var rgb_color = hexToRgb(hex_color);
        return luminanace(rgb_gb.r, rgb_gb.g, rgb_gb.b) / luminanace(rgb_color.r, rgb_color.g, rgb_color.b);
    }

    function MakeContrast($elements){
        $elements.each(function(){
            $element = $j(this);
            var origin_color = $element.css('color');
            //origin_color = origin_color.match(/^rgb\((\d+),\s*(\d+),\s*(\d+)\)$/);
            switch (origin_color){
                case 'rgb(119, 119, 119)':
                case 'rgb(201, 201, 201)':
                case 'rgb(204, 204, 204)':
                case 'rgb(153, 153, 153)':
                    $element.addClass('contrasted_grey_on_white');
                break;
                case 'rgb(241, 92, 34)':
                case 'rgb(255, 102, 0)':
                    $element.addClass('contrasted_orange_on_white');
                break;
                case 'rgb(0, 186, 221)':
                case 'rgb(0, 204, 255)':
                    $element.addClass('contrasted_blue_on_white');
                break;
            }
            var origin_color = $element.css('background-color');
            switch (origin_color){
                case 'rgb(0, 186, 221)':
                case 'rgb(0, 204, 255)':
                    $element.addClass('contrasted_white_on_blue');
                break;
            }
        });
    }

    function RemoveContrast(){
        $j('.contrasted_grey_on_white').removeClass('contrasted_grey_on_white');
        $j('.contrasted_orange_on_white').removeClass('contrasted_orange_on_white');
        $j('.contrasted_blue_on_white').removeClass('contrasted_blue_on_white');
        $j('.contrasted_white_on_blue').removeClass('contrasted_white_on_blue');
    }

	$j('.sizes a').click(function(){
//        log(GetRatioForColor('#ffffff', '#00badd'));
//        log(GetRatioForColor('#ffffff', '#005d6e'));
		$j('.sizes a').removeClass('active');
		$j(this).addClass('active');
		var font_size = $j(this).attr('id');
		console.log(font_size);
		switch(font_size){
			case 'font-large':
				if(!$j('body').hasClass('fontsize')){
					$j('body').addClass('fontsize');
					$j('body').addClass('contrasted');
                    MakeContrast($j('*'));
				}
				break;
			case 'font-normal':
				if($j('body').hasClass('fontsize')){
					$j('body').removeClass('fontsize');
                    $j('body').removeClass('contrasted');
                    RemoveContrast();
				}
				break;

		}
	    jQuery.post(
            'http://www.enosh.org.il/wp-admin/admin-ajax.php',
            {
                'action': 'myajax-access-set',
                'font-size':   font_size

            },
            function(response){
            }
        );
	});

    $j(document).ready(function(){
        if(jQuery('body').hasClass('contrasted')){
            MakeContrast(jQuery('*'));
        }
    });

